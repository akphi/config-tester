{
  "name": "config-tester",
  "private": true,
  "engines": {
    "node": ">=14.3.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build:tsc": "tsc -b .",
    "check-copyright-headers": "node ./scripts/checkCopyrightHeaders.js",
    "clean:build": "rimraf './packages/*/build'",
    "dev": "yarn workspace @akphi/app1 start",
    "dev:tsc": "tsc -b --watch .",
    "setup": "node ./scripts/bootstrap.js",
    "start": "yarn dev",
    "test": "echo \"TODO: run test\"",
    "update": "yarn upgrade-interactive --latest"
  },
