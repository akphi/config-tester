{
  "name": "config-tester",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "yarn clean && lerna run build --stream",
    "build:ts": "tsc --build",
    "check:copyright": "node ./scripts/checkCopyrightHeaders.js",
    "check:update": "yarn upgrade-interactive --latest",
    "clean": "rimraf \"build\" \"./packages/*/build\"",
    "dev": "yarn workspace @akphi/app1 start",
    "dev:ts": "tsc --build --watch",
    "lint": "yarn lint:js && yarn lint:style",
    "lint:js": "eslint --cache --cache-location ./build/.eslintcache --ext .js,.ts,.tsx scripts packages/*/src",
    "lint:package-json": "sort-package-json",
    "lint:style": "stylelint --cache --cache-location ./build/.stylelintcache \"packages/*/src/**/*.scss\" \"packages/*/src/**/*.css\"",
    "setup": "node ./scripts/bootstrap.js",
    "start": "yarn dev",
    "test": "jest",
    "type-check": "lerna run type-check --stream"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{html,md,mdx,scss,css}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "package.json": [
      "yarn lint:package-json"
    ]
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "dependencies": {
    "@material-ui/core": "4.11.2",
    "@types/lodash": "4.14.165",
    "@types/node": "14.14.14",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "lodash": "4.17.20",
    "mobx": "6.0.4",
    "mobx-react-lite": "3.1.6",
    "monaco-editor": "0.21.2",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-react": "7.12.10",
    "@babel/preset-typescript": "7.12.7",
    "@pmmmwh/react-refresh-webpack-plugin": "0.4.3",
    "@typescript-eslint/eslint-plugin": "4.11.0",
    "@typescript-eslint/parser": "4.11.0",
    "autoprefixer": "10.1.0",
    "babel-loader": "8.2.2",
    "chalk": "4.1.0",
    "circular-dependency-plugin": "5.2.2",
    "cross-env": "7.0.3",
    "css-loader": "5.0.1",
    "cssnano": "4.1.10",
    "eslint": "7.16.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "html-webpack-plugin": "5.0.0-beta.1",
    "husky": "4.3.6",
    "lerna": "3.22.1",
    "lint-staged": "10.5.3",
    "mini-css-extract-plugin": "1.3.3",
    "monaco-editor-webpack-plugin": "2.0.0",
    "postcss": "8.2.1",
    "postcss-loader": "4.1.0",
    "postcss-scss": "3.0.4",
    "prettier": "2.2.1",
    "react-refresh": "0.9.0",
    "resolve": "1.19.0",
    "rimraf": "3.0.2",
    "sass": "1.30.0",
    "sass-loader": "10.1.0",
    "semver": "7.3.4",
    "sort-package-json": "1.48.0",
    "stylelint": "13.8.0",
    "stylelint-config-standard": "20.0.0",
    "typescript": "4.1.3",
    "webpack": "5.11.0",
    "webpack-cli": "4.2.0",
    "webpack-dev-server": "3.11.0"
  },
  "engines": {
    "node": ">=14.3.0"
  }
}
