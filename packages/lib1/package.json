{
  "name": "@akphi/lib1",
  "version": "0.0.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/akphi/config-tester",
    "directory": "packages/lib1"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "yarn clean && yarn build:tsc",
    "build:tsc": "tsc --project ./tsconfig.build.json",
    "clean": "rimraf \"build\" \"lib\"",
    "dev": "tsc --watch --preserveWatchOutput",
    "prepublish": "yarn check:publish",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "jest": "26.6.3",
    "rimraf": "3.0.2",
    "typescript": "4.1.3"
  }
}
