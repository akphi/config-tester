{
  "name": "@akphi/component1",
  "version": "0.0.14",
  "homepage": "https://github.com/akphi/config-tester/tree/master/packages/component1",
  "bugs": {
    "url": "https://github.com/akphi/config-tester/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akphi/config-tester.git",
    "directory": "packages/component1"
  },
  "license": "MIT",
  "sideEffects": false,
  "main": "lib/index.cjs",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "yarn clean && yarn build:tsc && yarn build:rollup",
    "build:analyze": "cross-env NODE_ENV=development BUILD_MODE=analyze rollup --config",
    "build:rollup": "cross-env NODE_ENV=production rollup --config",
    "build:tsc": "tsc --project ./tsconfig.build.json",
    "clean": "rimraf \"build\" \"lib\"",
    "dev": "npm-run-all --parallel dev:ts dev:rollup",
    "dev:advanced": "cross-env NODE_ENV=development DEVELOPMENT_MODE=advanced rollup --config --watch",
    "dev:fast": "cross-env NODE_ENV=development DEVELOPMENT_MODE=fast rollup --config --watch",
    "dev:rollup": "cross-env NODE_ENV=development rollup --config --watch",
    "dev:ts": "tsc --emitDeclarationOnly --watch --preserveWatchOutput",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@akphi/lib1": "workspace:*",
    "@material-ui/core": "4.11.2",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@akphi/dev-utils": "workspace:*",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "cross-env": "7.0.3",
    "jest": "26.6.3",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "rollup": "2.36.1",
    "typescript": "4.1.3"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.11.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  }
}
