{
  "name": "@akphi/component1",
  "version": "0.0.3",
  "license": "MIT",
  "sideEffects": false,
  "main": "lib/index.cjs",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "yarn clean && yarn build:tsc && yarn build:rollup",
    "build:analyze": "cross-env NODE_ENV=development BUILD_MODE=analyze rollup --config",
    "build:rollup": "cross-env NODE_ENV=production rollup --config",
    "build:tsc": "tsc --project ./tsconfig.build.json",
    "clean": "rimraf \"build\" \"lib\"",
    "dev": "npm-run-all --parallel dev:ts dev:rollup",
    "dev:advanced": "cross-env NODE_ENV=development DEVELOPMENT_MODE=advanced rollup --config --watch",
    "dev:fast": "cross-env NODE_ENV=development DEVELOPMENT_MODE=fast rollup --config --watch",
    "dev:rollup": "cross-env NODE_ENV=development rollup --config --watch",
    "dev:ts": "tsc --emitDeclarationOnly --watch --preserveWatchOutput",
    "prepublish": "yarn check:license",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@akphi/lib1": "0.0.3",
    "@material-ui/core": "4.11.2",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@akphi/dev-utils": "0.0.1",
    "jest": "26.6.3"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.11.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  }
}
