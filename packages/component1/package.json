{
  "name": "@akphi/component1",
  "version": "0.0.0",
  "main": "build/lib/index.js",
  "types": "build/lib/index.d.ts",
  "scripts": {
    "build": "yarn clean && yarn build:tsc && yarn build:webpack",
    "build:tsc": "tsc --project ./tsconfig.build.json",
    "build:webpack": "webpack --mode production",
    "clean": "rimraf './build'",
    "dev:advanced": "cross-env DEVELOPMENT_MODE=advaned webpack --mode development",
    "dev:fast": "cross-env DEVELOPMENT_MODE=fast webpack --mode development",
    "dev:type-check": "tsc --noEmit --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@akphi/lib1": "^0.0.0"
  }
}
