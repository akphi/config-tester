{
  "name": "@akphi/component1",
  "version": "0.0.0",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "yarn clean && yarn build:tsc && yarn build:webpack",
    "build:analyze": "cross-env NODE_ENV=production webpack --mode production --analyze",
    "build:tsc": "tsc --project ./tsconfig.build.json",
    "build:webpack": "cross-env NODE_ENV=production webpack --mode production",
    "clean": "rimraf \"build\" \"lib\"",
    "dev": "cross-env NODE_ENV=development webpack --mode development --watch",
    "dev:advanced": "cross-env NODE_ENV=development DEVELOPMENT_MODE=advaned webpack --mode development --hot --watch",
    "dev:fast": "cross-env NODE_ENV=development DEVELOPMENT_MODE=fast webpack --mode development --hot --watch",
    "dev:ts": "tsc --emitDeclarationOnly --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@akphi/lib1": "^0.0.0",
    "@material-ui/core": "4.11.2",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@akphi/dev-utils": "^0.0.0"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.11.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  }
}
