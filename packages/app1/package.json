{
  "name": "@akphi/app1",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "yarn clean && yarn build:tsc && yarn build:webpack",
    "build:tsc": "tsc --project ./tsconfig.build.json",
    "build:webpack": "webpack --mode production",
    "clean": "rimraf './build'",
    "dev": "webpack serve --mode development --hot",
    "dev:advanced": "cross-env DEVELOPMENT_MODE=advaned webpack serve --mode development --hot",
    "dev:fast": "cross-env DEVELOPMENT_MODE=fast webpack serve --mode development --hot",
    "dev:type-check": "tsc --noEmit --watch",
    "lint": "eslint --config ./.eslintrc-advanced.js .",
    "lint:fix": "eslint --config ./.eslintrc-advanced.js --fix .",
    "serve": "npx http-server ./build/web -p 3000 -a localhost -g --cors -o /ide",
    "setup": "node ../../scripts/createAppConfig.js",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@akphi/component1": "^0.0.0"
  },
  "devDependencies": {
    "@akphi/dev-utils": "^0.0.0"
  }
}
